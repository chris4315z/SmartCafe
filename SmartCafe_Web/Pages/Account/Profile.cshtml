@page
@model SmartCafe_Web.Pages.Account.ProfileModel
@{
}
<div class="row text-center">
    <div class="col-12">
        <label class="form-label fs-1">Profile</label>
    </div>
</div>
<div class="row my-2">
    <div class="col-2">
        <label asp-for="UserProfile.FirstName" class="form-label"></label>
    </div>
    <div class="col-10">
        <label class="form-label">@Model.UserProfile.FirstName</label>
    </div>
</div>
<div class="row my-2">
    <div class="col-2">
        <label asp-for="UserProfile.LastName" class="form-label"></label>
    </div>
    <div class="col-10">
        <label class="form-label">@Model.UserProfile.LastName</label>
    </div>
</div>
<div class="row my-2">
    <div class="col-2">
        <label asp-for="UserProfile.Email" class="form-label"></label>
    </div>
    <div class="col-10">
        <label class="form-label">@Model.UserProfile.Email</label>
    </div>
</div>
<div class="row my-2">
    <div class="col-2">
        <label asp-for="UserProfile.AccountType" class="form-label"></label>
    </div>
    <div class="col-10">
        <label class="form-label">@Model.UserProfile.AccountType</label>
    </div>
</div>
<div class="row my-2">
    <div class="col-2">
        <label asp-for="UserProfile.LastLoginTime" class="form-label"></label>
    </div>
    <div class="col-10">
        <label class="form-label">@Model.UserProfile.LastLoginTime</label>
    </div>
</div>